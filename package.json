{
  "name": "Automation Test Suite",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "prettier": "prettier --write .",
    "cy:run_spec": "npx cypress run --spec 'cypress/e2e/core'",
    "cy:run_browsers_fire": "npm run cy:run_spec -- --browser chrome",
    "cy:run_browsers": "npm run cy:run_spec -- --browser firefox & npm run cy:run_spec -- --browser chrome & npm run cy:run_spec -- --browser electron --env allure=true --headed",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.35.2",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cypress": "^12.4.1",
    "cypress-xpath": "^2.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "playwright-webkit": "^1.30.0",
    "typescript": "^4.9.5"
  }
}
